}
.addressBox_1{
  width: 600rpx;
}
.cellplaceholder{
  color: #999;
}
.dividerBox{
  padding-left: 30rpx;
}
.addressBoxItem{
  padding: 20rpx 0;
}

.addressBoxItem_1{
  font-size: 25rpx;
  color: #666;
  margin-top: 10rpx;
}


